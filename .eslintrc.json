{
	"parserOptions": {
		"ecmaVersion": 6
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-console": 1,
		"no-unused-vars": 0,
		"block-scoped-var": 0,
		"consistent-return": 2,
		"curly": 2,
		"no-return-assign": 2,
		"no-void": 2,
		"no-with": 2,
		"no-shadow": 2,
		"no-useless-escape": 0
	},
	"env": {
		"browser": true
	},
	"root": true
}